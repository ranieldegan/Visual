<%- include('partials/header'); %>
  <h1>Visual</h1>  
  <% if (locals.errors && locals.errors.login) { %>
    <% locals.errors.login.forEach(function(error) { %>
      <p class="alert alert-primary" role="alert"><%= error %></p>
    <% }) %>
  <% } %>
  <form action="/login" method="POST">
      <div class="form-group">
        <label for="username">Username:</label>
        <input type="email" class="form-control" id="usr" placeholder="Enter username" name="email">
      </div>
      <div class="form-group">
        <label for="pwd">Password:</label>
        <input type="password" class="form-control" id="pwd" placeholder="Enter password" name="password">
      </div>
      <div class="form-group form-check">
        <label class="form-check-label">
          <input class="form-check-input" type="checkbox" name="remember"> Remember me
      </div>
      <button type="submit" value="Submit">
    </form>
    </div> <!-- end .row --> 
  </div><!-- end .container-fluid -->
</div><!-- end #main -->

<div class="outform">
  

</div>

<%- include('partials/footer'); %>